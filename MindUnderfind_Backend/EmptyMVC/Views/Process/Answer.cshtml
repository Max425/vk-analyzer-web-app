@model EmptyMVC.Views.Home.AnswerModel
@{
    string VkId = $"VkId: {Model.VkId}";
    string ProcessType = $"ProcessType: {Model.ProcessType}";
    string ComVkId = $"ComVkId: {Model.ComVkId}";
}
<!DOCTYPE html>
<html>
<head>
    <title>Ответ на запрос</title>
    <meta charset="utf-8" />
</head>
<body>
    <p>@VkId</p>
    <p>@ProcessType</p>
    <p>@ComVkId</p>
    <h2>GROUPS</h2>
    @foreach (var group in Model.GroupsArr)
    {
        <h4>@group</h4>
    }

    <h2>USERS</h2>
    @foreach (var user in Model.UsersArr)
    {
            <h4>@user</h4>
    }
</body>
</html>
 